{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": [
                "OPTIONS",
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ],
            "dest": "api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "buildCommand": "echo 'Building Pharam Backend API'",
    "outputDirectory": "./"
}